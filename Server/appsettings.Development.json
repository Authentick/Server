{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Hangfire": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "AuthDb": "Host=localhost;Database=db;Username=postgres;Password=example",
    "HangfireDb": "Host=localhost;Database=db;Username=postgres;Password=example",
    "KeyStorageDb": "Host=localhost;Database=db;Username=postgres;Password=example"
  },
  "SMTP_HOST": "localhost",
  "SMTP_PORT": "1025",
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:5001"
      }
    }
  },
  "ReverseProxy": {
    "Routes": [
      {
        "RouteId": "route1",
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    ],
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "cluster1/destination1": {
            "Address": "https://example.com/"
          }
        }
      }
    }
  }
}
